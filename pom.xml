  <build>
    <resources>
		<resource>
			<directory>resources</directory>
			<targetPath>resources</targetPath>
			<includes>
				<include>**/*.csv</include>
				<include>**/*.bin</include>
			</includes>
		</resource>
	</resources>
    <sourceDirectory>src</sourceDirectory>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version>
        <configuration>
          <source>17</source>
          <target>17</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
